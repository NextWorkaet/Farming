    local args = {
        [1] = "MainMap",
        [2] = game:GetService("Players").LocalPlayer,
        [3] = "Default"
    }
    
    game:GetService("ReplicatedStorage"):WaitForChild("API"):WaitForChild("LocationAPI/SetLocation"):FireServer(unpack(args))
end
